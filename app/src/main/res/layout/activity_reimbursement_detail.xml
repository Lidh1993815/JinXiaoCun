<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/_eeeeee"
    android:orientation="vertical"
    tools:context="com.qianmo.jinxiaocun.fu.activity.ReimbursementDetailActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/rv_add_reim"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_above="@+id/reim_detail_list">

        </android.support.v7.widget.RecyclerView>

        <include
            layout="@layout/progress_detail" />
    </LinearLayout>
</ScrollView>
